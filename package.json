{
  "name": "wolfpack",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "web": "./node_modules/node-dev/bin/node-dev ./bin/web",
    "worker": "./node_modules/node-dev/bin/node-dev ./bin/worker",
    "build": "./node_modules/gulp/bin/gulp.js",
    "postinstall": "./node_modules/bower/bin/bower install",
    "deploy web": "./node_modules/naught/lib/main.js deploy /var/run/wolfpack-web.ipc",
    "deploy worker": "./node_modules/naught/lib/main.js deploy /var/run/wolfpack-worker.ipc",
    "start web": "./node_modules/naught/lib/main.js start --ipc-file /var/run/wolfpack-web.ipc --log /var/log/wolfpack/naught-web.log --stdout /var/log/wolfpack/stdout-web.log --stderr /var/log/wolfpack/stderr-web.log bin/web",
    "start worker": "./node_modules/naught/lib/main.js start --ipc-file /var/run/wolfpack-worker.ipc --log /var/log/wolfpack/naught-worker.log --stdout /var/log/wolfpack/stdout-worker.log --stderr /var/log/wolfpack/stderr-worker.log bin/worker",
    "stop web": "./node_modules/naught/lib/main.js /var/run/wolfpack-web.ipc",
    "stop worker": "./node_modules/naught/lib/main.js /var/run/wolfpack-worker.ipc"
  },
  "dependencies": {
    "body-parser": "~1.0.0",
    "bower": "^1.3.12",
    "cookie-parser": "~1.0.1",
    "debug": "^0.7.4",
    "dotenv": "^0.4.0",
    "express": "~4.2.0",
    "gulp": "^3.8.10",
    "gulp-less": "^1.3.7",
    "gulp-livereload": "^2.1.1",
    "gulp-minify-css": "^0.3.11",
    "gulp-plumber": "^0.6.6",
    "gulp-sourcemaps": "^1.2.8",
    "gulp-stylus": "^1.3.4",
    "gulp-watch": "^3.0.0",
    "jade": "~1.3.0",
    "lodash": "^2.4.1",
    "morgan": "~1.0.0",
    "naught": "^1.5.0",
    "nib": "^1.0.4",
    "static-favicon": "~1.0.0"
  },
  "devDependencies": {
    "node-dev": "^2.6.0"
  }
}
